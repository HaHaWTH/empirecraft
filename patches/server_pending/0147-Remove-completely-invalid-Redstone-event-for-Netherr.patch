index 7daf006..676a993 100644
--- a/src/main/java/net/minecraft/server/BlockBloodStone.java
+++ b/src/main/java/net/minecraft/server/BlockBloodStone.java
@@ -14,8 +14,8 @@ public class BlockBloodStone extends Block {
     }
 
     // CraftBukkit start
-    @Override
