Subject: [PATCH] Get rid of some undesired log lines

---
 src/main/java/net/minecraft/server/AdvancementDataPlayer.java | 2 +-
 src/main/java/net/minecraft/server/AutoRecipe.java            | 3 ++-
 src/main/java/net/minecraft/server/RecipeBookServer.java      | 2 +-
 3 files changed, 4 insertions(+), 3 deletions(-)

diff --git a/src/main/java/net/minecraft/server/AdvancementDataPlayer.java b/src/main/java/net/minecraft/server/AdvancementDataPlayer.java
index 79e11eebb7..b727ae19bf 100644
--- a/src/main/java/net/minecraft/server/AdvancementDataPlayer.java
+++ b/src/main/java/net/minecraft/server/AdvancementDataPlayer.java
@@ -164,7 +164,7 @@ public class AdvancementDataPlayer {
                         if (advancement == null) {
                             // CraftBukkit start
                             if (entry.getKey().getNamespace().equals("minecraft")) {
-                                AdvancementDataPlayer.LOGGER.warn("Ignored advancement '{}' in progress file {} - it doesn't exist anymore?", entry.getKey(), this.e);
+                                //AdvancementDataPlayer.LOGGER.warn("Ignored advancement '{}' in progress file {} - it doesn't exist anymore?", entry.getKey(), this.e); // EMC
                             }
                             // CraftBukkit end
                         } else {
diff --git a/src/main/java/net/minecraft/server/AutoRecipe.java b/src/main/java/net/minecraft/server/AutoRecipe.java
index dbc03cfc12..8201c1feb2 100644
--- a/src/main/java/net/minecraft/server/AutoRecipe.java
+++ b/src/main/java/net/minecraft/server/AutoRecipe.java
@@ -65,7 +65,8 @@ public class AutoRecipe<C extends IInventory> implements AutoRecipeAbstract<Inte
 
                 itemstack1.setCount(1);
                 if (!this.c.c(j, itemstack1)) {
-                    AutoRecipe.LOGGER.error("Can't find any space for item in the inventory");
+                    String type = this instanceof AutoRecipeFurnace ? "AutoRecipeFurnace" : "AutoRecipe"; // EMC
+                    AutoRecipe.LOGGER.error("Can't find any space for item in the inventory (" + type + "). Item: " + itemstack.getBukkitStack().toString()); // EMC
                 }
             }
 
diff --git a/src/main/java/net/minecraft/server/RecipeBookServer.java b/src/main/java/net/minecraft/server/RecipeBookServer.java
index 0203e26ec9..5c30eb4f2c 100644
--- a/src/main/java/net/minecraft/server/RecipeBookServer.java
+++ b/src/main/java/net/minecraft/server/RecipeBookServer.java
@@ -119,7 +119,7 @@ public class RecipeBookServer extends RecipeBook {
                 Optional<? extends IRecipe<?>> optional = this.l.a(minecraftkey);
 
                 if (!optional.isPresent()) {
-                    RecipeBookServer.LOGGER.error("Tried to load unrecognized recipe: {} removed now.", minecraftkey);
+                    //RecipeBookServer.LOGGER.error("Tried to load unrecognized recipe: {} removed now.", minecraftkey); // EMC
                 } else {
                     consumer.accept(optional.get());
                 }
