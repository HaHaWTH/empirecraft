+++ b/src/main/java/net/minecraft/server/RemoteControlSession.java
@@ -97,7 +97,7 @@ public class RemoteControlSession extends RemoteConnectionThread {
                 return;
             } finally {
                 this.e();
-                RemoteControlSession.LOGGER.info("Thread {} shutting down", this.b);
+                // RemoteControlSession.LOGGER.info("Thread {} shutting down", this.b); // Paper
                 this.a = false;
             }
 
