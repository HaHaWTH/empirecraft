 src/main/java/net/minecraft/server/RemoteConnectionThread.java | 2 +-
 src/main/java/net/minecraft/server/RemoteControlSession.java   | 2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/src/main/java/net/minecraft/server/RemoteConnectionThread.java b/src/main/java/net/minecraft/server/RemoteConnectionThread.java
index 10e3fff4588e6936c7884d6080c786cc03e4e33d..a41135618c3de85a3b8ad8812b21dcd30b33c785 100644
