From: Aikar <aikar@aikar.co>
Date: Sun, 19 May 2013 21:09:31 -0400
Subject: [PATCH] ArrowHitBlock Event

---
 src/main/java/net/minecraft/server/EntityArrow.java | 1 +
 1 file changed, 1 insertion(+)

diff --git a/src/main/java/net/minecraft/server/EntityArrow.java b/src/main/java/net/minecraft/server/EntityArrow.java
index 743a79d360..ccc365c83f 100644
--- a/src/main/java/net/minecraft/server/EntityArrow.java
+++ b/src/main/java/net/minecraft/server/EntityArrow.java
@@ -304,6 +304,7 @@ public abstract class EntityArrow extends Entity implements IProjectile {
             this.shake = 7;
             this.setCritical(false);
             if (!iblockdata.isAir()) {
+                if (new com.destroystokyo.paper.event.entity.ArrowHitBlockEvent((org.bukkit.entity.Arrow)this.getBukkitEntity(), world.getWorld().getBlockAt(blockposition.getX(), blockposition.getY(), blockposition.getZ())).callEvent()) // Paper
                 this.az.a(this.world, blockposition, (Entity) this);
             }
         }
