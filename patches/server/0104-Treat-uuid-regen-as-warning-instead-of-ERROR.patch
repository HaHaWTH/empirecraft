From fad684dc8bc68a4725070f1c15bc9b6fa608badc Mon Sep 17 00:00:00 2001
From: Aikar <aikar@aikar.co>
Date: Sat, 21 Jul 2018 16:33:41 -0400
Subject: [PATCH] Treat uuid regen as warning instead of ERROR

---
 src/main/java/net/minecraft/server/Chunk.java | 8 ++++----
 1 file changed, 4 insertions(+), 4 deletions(-)

diff --git a/src/main/java/net/minecraft/server/Chunk.java b/src/main/java/net/minecraft/server/Chunk.java
index 294560b977..b7c5c38915 100644
--- a/src/main/java/net/minecraft/server/Chunk.java
+++ b/src/main/java/net/minecraft/server/Chunk.java
@@ -875,13 +875,13 @@ public class Chunk {
                         switch (mode) {
                             case REGEN: {
                                 entity.setUUID(UUID.randomUUID());
-                                logger.error("Duplicate UUID found used by " + other);
-                                logger.error("Regenerated a new UUID for " + entity);
+                                logger.warn("[DUPE-UUID] Duplicate UUID found used by " + other);
+                                logger.warn("[DUPE-UUID] Regenerated a new UUID for " + entity);
                                 break;
                             }
                             case DELETE: {
-                                logger.error("Duplicate UUID found used by " + other);
-                                logger.error("Deleting duplicate entity " + entity);
+                                logger.warn("[DUPE-UUID] Duplicate UUID found used by " + other);
+                                logger.warn("[DUPE-UUID] Deleting duplicate entity " + entity);
                                 entity.die();
                                 iterator.remove();
                                 break;
-- 
