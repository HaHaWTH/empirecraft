From 167bd9f3f3d011dcfa12814591ff55238b066632 Mon Sep 17 00:00:00 2001
From: Aikar <aikar@aikar.co>
Date: Thu, 4 Jul 2013 21:05:05 -0400
Subject: [PATCH] Inventory Meta API

---
 .../java/org/bukkit/craftbukkit/inventory/CraftInventory.java | 11 ++++++++++-
 1 file changed, 10 insertions(+), 1 deletion(-)

diff --git a/src/main/java/org/bukkit/craftbukkit/inventory/CraftInventory.java b/src/main/java/org/bukkit/craftbukkit/inventory/CraftInventory.java
index 577e210..9ede0fc 100644
--- a/src/main/java/org/bukkit/craftbukkit/inventory/CraftInventory.java
+++ b/src/main/java/org/bukkit/craftbukkit/inventory/CraftInventory.java
@@ -28,7 +28,16 @@ import org.bukkit.Material;
 
 public class CraftInventory implements Inventory {
     protected final IInventory inventory;
-
+	// EMC start
+	private static HashMap<IInventory, HashMap<String, Object>> meta = new HashMap<IInventory, HashMap<String, Object>>(8);
+	public HashMap<String, Object> getMeta() {
+		HashMap<String, Object> metaMap = meta.get(inventory);
+		if (metaMap == null) {
+			metaMap = new HashMap<String, Object>(8);
+			meta.put(inventory, metaMap);
+		}
+		return metaMap;
+	} // EMC end
     public CraftInventory(IInventory inventory) {
         this.inventory = inventory;
     }
-- 
