+
+package com.empireminecraft;
+
+public interface MinecraftKey {
+    String getGroup();
+    String getName();
+}
diff --git a/src/main/java/org/bukkit/Bukkit.java b/src/main/java/org/bukkit/Bukkit.java
index eb6fb2c..2189b47 100644
--- a/src/main/java/org/bukkit/Bukkit.java
+++ b/src/main/java/org/bukkit/Bukkit.java
@@ -44,10 +44,22 @@ import org.bukkit.generator.ChunkGenerator;
 import org.bukkit.inventory.ItemFactory;
 import org.bukkit.inventory.meta.ItemMeta;
 
+import javax.annotation.Nonnull;
+
 /**
  * Represents the Bukkit core, for version and Server singleton handling
  */
 public final class Bukkit {
+
+    // EMC start
+    public static com.empireminecraft.MinecraftKey createKey(@Nonnull String name) {
+        return server.createKey(name);
+    }
+    public static com.empireminecraft.MinecraftKey createKey(@Nonnull String group, @Nonnull String name) {
+        return server.createKey(group, name);
+    }
+    // EMC end
+
     private static Server server;
 
     /**
diff --git a/src/main/java/org/bukkit/Server.java b/src/main/java/org/bukkit/Server.java
index 0850009..863f41d 100644
--- a/src/main/java/org/bukkit/Server.java
+++ b/src/main/java/org/bukkit/Server.java
@@ -12,6 +12,7 @@ import java.util.Set;
 import java.util.UUID;
 import java.util.logging.Logger;
 
+import com.empireminecraft.MinecraftKey;
 import org.bukkit.Warning.WarningState;
 import org.bukkit.boss.BarColor;
 import org.bukkit.boss.BarFlag;
@@ -45,11 +46,20 @@ import org.bukkit.generator.ChunkGenerator;
 import org.bukkit.inventory.ItemFactory;
 import org.bukkit.inventory.meta.ItemMeta;
 
+import javax.annotation.Nonnull;
+
 /**
  * Represents a server implementation.
  */
 public interface Server extends PluginMessageRecipient {
 
+    // EMC start
+    public default MinecraftKey createKey(@Nonnull String name) {
+        return createKey("minecraft", name);
+    }
+    public MinecraftKey createKey(@Nonnull String group, @Nonnull String name);
+    // EMC end
+
     /**
      * Used for all administrative messages, such as an operator using a
      * command.
