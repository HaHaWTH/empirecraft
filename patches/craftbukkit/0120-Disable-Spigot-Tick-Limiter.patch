From: Aikar <aikar@aikar.co>
Date: Wed, 29 Jul 2015 01:17:36 -0400
Subject: [PATCH] Disable Spigot Tick Limiter

It sucks.
---
 src/main/java/net/minecraft/server/World.java | 10 +++-------
 1 file changed, 3 insertions(+), 7 deletions(-)

diff --git a/src/main/java/net/minecraft/server/World.java b/src/main/java/net/minecraft/server/World.java
index acda786..fd3e7bd 100644
--- a/src/main/java/net/minecraft/server/World.java
+++ b/src/main/java/net/minecraft/server/World.java
@@ -1428,10 +1428,8 @@ public abstract class World implements IBlockAccess {
         // CraftBukkit start - Use field for loop variable
         org.spigotmc.timings.TimingHistory.entityTicks += this.entityList.size(); // Spigot
         int entitiesThisCycle = 0;
-        if (tickPosition < 0) tickPosition = 0;
-        for (entityLimiter.initTick();
-                entitiesThisCycle < entityList.size() && (entitiesThisCycle % 10 == 0 || entityLimiter.shouldContinue());
-                tickPosition++, entitiesThisCycle++) {
+        //if (tickPosition < 0) tickPosition = 0; // EMC
+        for (tickPosition = 0; tickPosition < entityList.size(); tickPosition++) { // EMC
             tickPosition = (tickPosition < entityList.size()) ? tickPosition : 0;
             entity = (Entity) this.entityList.get(this.tickPosition);
             // CraftBukkit end
@@ -1508,9 +1506,7 @@ public abstract class World implements IBlockAccess {
         // Spigot start
         timings.tileEntityTick.startTiming();
         int tilesThisCycle = 0;
-        for (tileLimiter.initTick();
-                tilesThisCycle < tileEntityList.size() && (tilesThisCycle % 10 == 0 || tileLimiter.shouldContinue());
-                tileTickPosition++, tilesThisCycle++) {
+        for (tileTickPosition = 0; tileTickPosition < tileEntityList.size(); tileTickPosition++) { // EMC
             tileTickPosition = (tileTickPosition < tileEntityList.size()) ? tileTickPosition : 0;
             TileEntity tileentity = (TileEntity) this.tileEntityList.get(tileTickPosition);
             // Spigot start
