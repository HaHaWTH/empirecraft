From 3ca2487c12251b7903999e5e9187e00952d81de0 Mon Sep 17 00:00:00 2001
From: Aikar <aikar@aikar.co>
Date: Thu, 3 Sep 2015 00:36:38 -0400
Subject: [PATCH] Prevent double shading packages

---
 pom.xml | 4 ++++
 1 file changed, 4 insertions(+)

diff --git a/pom.xml b/pom.xml
index 8bdba5e..47c9e6c 100644
--- a/pom.xml
+++ b/pom.xml
@@ -267,11 +267,15 @@
                                     <shadedPattern>org.bukkit.craftbukkit.v${minecraft_version}</shadedPattern>
                                     <excludes>
                                         <exclude>org.bukkit.craftbukkit.Main*</exclude>
+                                        <exclude>org.bukkit.craftbukkit.v${minecraft_version}.*</exclude>
                                     </excludes>
                                 </relocation>
                                 <relocation>
                                     <pattern>net.minecraft.server</pattern>
                                     <shadedPattern>net.minecraft.server.v${minecraft_version}</shadedPattern>
+                                    <excludes>
+                                        <exclude>net.minecraft.server.v${minecraft_version}.*</exclude>
+                                    </excludes>
                                 </relocation>
                             </relocations>
                         </configuration>
-- 
2.6.3

